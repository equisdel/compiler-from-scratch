compatibility_test
begin

    uinteger FUN fun1 (hexa p)
    begin

        uinteger u1,u2;                 ## Declaracion
        u1,u2 := 1,2;                   ## Asignacion Múltiple
        outf(u1+u2);                    ## Suma compatible + Impresión por pantalla

        uinteger FUN fun2 (uinteger x)
        begin
            outf (x);
            ret (x);    
        end

        p := u1+u2;     ## Conversión implícita
        ret(p);         ## Llamado a función
    end

    single r;
    r := 0.0;

end
