uinteger fun testeveryverylarge(uinteger intval)
begin
    if intval > 10 then
        goto SALTITO@;
    else
        RET (intval * intval); 
    end_if;
end
uinteger aux;
aux := testeveryverylarge(5);
SALTITO@
outf(aux);

repeat begin
aux := aux + 1;
end
until (aux = 10);

end
