## Este programa de prueba tiene errores de declaraci√≥n de variables y funciones.

programap3
begin

single sing1 , sing2;

outf([impresion]);

sing1,sing2 := 1.2s+2 , .2s-2;

uinteger fun testeveryverylarge(uinteger intval)
begin
    if (intval < 10) then
        goto SALTITO@;
    end_if;
end
uinteger aux;
aux := testeveryverylarge(5);
SALTITO@
outf(aux);

repeat begin
    aux := aux + 5;
end
 until (aux < 10);

end
